    "type": "library",
    "require": {
        "php": "^7.0",
        "ext-json": "^1.4",
        "psr/log": "^1.1"
    },
    "suggest": {
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "*"
    },
    "autoload": {
      "psr-4": {
          "Manticoresearch\\" : "src/Manticoresearch/"
      }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Adrian Nuta",
            "email": "adriannuta@gmail.com"
        }
    ]
}
